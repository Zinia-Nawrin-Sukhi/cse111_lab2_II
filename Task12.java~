    import java.util.Scanner;
    public class Task1
    {
     public static void main(String[]args) 
     {
        Scanner sc=new Scanner(System.in);
     int a[]=new int[5];
     int max;
     int index;
     for(index=0;index<a.length;index++)
            {
              System.out.println("please enter a number");
                a[index] = sc.nextInt();
            }

            max = a[0];
            for( index = 0; index < a.length; index++)
            {
              if(max < a[index])
                {
                max = a[index];
                }
            }
            System.out.println(max);

        }

    }